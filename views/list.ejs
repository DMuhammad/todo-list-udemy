<%- include("header") -%> 
    <div id="heading" class="box">
        <h1> <%= title %> </h1>
    </div>
    
    <div class="box">
        <% for( let i = 0; i < listItem.length; i++ ) { %>
            <div class="item">
                <input type="checkbox">
                <p> <%= listItem[i] %>  </p>
            </div>
        <% } %>
        <form action="/" method="post">
            <input type="text" name="newItem" placeholder="Kegiatan" autocomplete="off">
            <button type="submit" name="button" value="<%= title %>">+</button>
        </form>
    </div>
<%- include("footer") -%> 